{
    "appName": "dashboard-api",
    "instanceCount": 1,
    "notExposeAsWebApp": false,
    "containerHttpPort": "$CODA_DASHBOARD_API_PORT",
    "forceSsl": false,
    "envVars": [
        {
            "key": "CODA_HUB_API_URL",
            "value": "$CODA_HUB_API_URL"
        },
        {
            "key": "CODA_AUTH_SERVICE_URL",
            "value": "$CODA_AUTH_SERVICE_URL"
        },
        {
            "key": "CODA_DASHBOARD_APP_URL",
            "value": "$CODA_DASHBOARD_APP_URL"
        },
        {
            "key": "CODA_DASHBOARD_API_AUTH_REALM",
            "value": "$CODA_DASHBOARD_API_AUTH_REALM"
        },
        {
            "key": "CODA_DASHBOARD_API_AUTH_SESSION_MEMORY_SECRET",
            "value": "$CODA_DASHBOARD_API_AUTH_SESSION_MEMORY_SECRET"
        },
        {
            "key": "CODA_DASHBOARD_API_AUTH_CLIENT_ID",
            "value": "$CODA_DASHBOARD_API_AUTH_CLIENT_ID"
        },
        {
            "key": "CODA_DASHBOARD_API_AUTH_CLIENT_SECRET",
            "value": "$CODA_DASHBOARD_API_AUTH_CLIENT_SECRET"
        }
    ]
}